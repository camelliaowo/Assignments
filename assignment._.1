int calculator() {

        printf(" Glad to see you are using our calculator program <3 !\n\n ");

        //int numbers=0;

        //printf("Notes: IT CAN ONLY MAINTAIN 4 NUMBERS ! \n\n");

        //numbers asking and operation taking

        int firstNumber;

        int secondNumber;

        //int thirdNumber;

        //int fourthNumber;

        //char betweenThirdAndFourth;

        //char betweenSecondAndThird;

        char betweenFirstAndSecond;

        int answerFirstAndSecond;

        //int answerSecondAndThird;

        //int answerThirdAndFourth;

        //printf("How many number do you have to calculate : ");

        //scanf("%d", &numbers);

        // printf("\n\n");

        // two numbers calculation



        printf("Enter the first number : ");

        scanf("%d", &firstNumber);

        printf("\n\n");

        printf ("Enter the second number : ");

        scanf("%d", &secondNumber);

        printf("\n\n");

        printf("Enter the operation +, - , * , / : ");

        scanf(" %c",&betweenFirstAndSecond);

        printf("\n\n");


    switch(betweenFirstAndSecond){

        case '+':

            answerFirstAndSecond= addFun(firstNumber, secondNumber);

            printf(" Result= %d\n\n", answerFirstAndSecond);

            break;

        case '-':

            answerFirstAndSecond= subtractFun(firstNumber, secondNumber);

            printf(" Result= %d\n\n", answerFirstAndSecond);

            break;

        case '*':

            answerFirstAndSecond= multiplyFun(firstNumber,secondNumber);

            printf(" Result= %d\n\n", answerFirstAndSecond);


            break;

        case '/':

            answerFirstAndSecond= divideFun(firstNumber,secondNumber);

            printf(" Result= %d\n\n", answerFirstAndSecond);

            break;

        default :

            printf("Syntax Error!");

            printf("\n\n");

            return answerFirstAndSecond;

        }

        }

int addFun (int first, int second){

    int answerFirstAndSecond = first + second;

    return answerFirstAndSecond;
}

int subtractFun(int first, int second){

    int answerFirstAndSecond = first - second;

    return answerFirstAndSecond;

}

int multiplyFun (int first, int second){

    int answerFirstAndSecond = first * second;

    return answerFirstAndSecond;
}

int divideFun(int first, int second){

    int answerFirstAndSecond = first / second;

    return answerFirstAndSecond;

}
